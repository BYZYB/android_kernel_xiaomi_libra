&pm8994_mpps {
	mpp@a000 { /* MPP 1 */
		status = "disabled";
	};

	mpp@a100 { /* MPP 2 */
	};

	mpp@a200 { /* MPP 3 */
	};

	mpp@a300 { /* MPP 4 */
	};

	mpp@a400 { /* MPP 5 */
	};

	mpp@a500 { /* MPP 6 */
	};

	mpp@a600 { /* MPP 7 */
	};

	mpp@a700 { /* MPP 8 */
	};
};

&pmi8994_mpps {
	mpp@a000 { /* MPP 1 */
	};

	mpp@a100 { /* MPP 2 */
	};

	mpp@a200 { /* MPP 3 */
	};

	mpp@a300 { /* MPP 4 */
	};
};

&msm_vidc {
	qcom,use-non-secure-pil;
	qcom,fw-bias = <0xf500000>;
};

&blsp2_uart2 {
	status = "ok";
};

&msm_cpufreq {
	qcom,cpufreq-table-0 =
		<  199200 >,
		<  302400 >,
		<  384000 >,
		<  600000 >,
		<  691200 >,
		<  768000 >,
		<  844800 >,
		<  921600 >,
		<  940800 >;

	qcom,cpufreq-table-4 =
		<  199200 >,
		<  302400 >,
		<  384000 >,
		<  600000 >,
		<  691200 >,
		<  768000 >,
		<  844800 >,
		<  921600 >;
